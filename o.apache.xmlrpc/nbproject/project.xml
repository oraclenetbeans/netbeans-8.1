                </dependency>
                <dependency>
                    <code-name-base>org.apache.ws.commons.util</code-name-base>
                    <compile-dependency/>
                    <run-dependency/>
                </dependency>
                <dependency>
                    <code-name-base>org.apache.commons.logging</code-name-base>
