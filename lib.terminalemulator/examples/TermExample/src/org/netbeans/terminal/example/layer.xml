        <folder name="BuildProject">    <!-- aka Run -->
            <file name="ShellTermAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-terminal-example-ShellTermAction.instance"/>
                <attr name="position" intvalue="3"/>
