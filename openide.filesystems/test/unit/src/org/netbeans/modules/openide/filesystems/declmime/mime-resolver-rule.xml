<?xml version="1.0"?>
<MIME-resolver>
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-ant+xml">
            <xml-rule>
                <element name="project">
                    <attr name="default"/>
                </element>
            </xml-rule>
        </resolver>
    </file>
    <file>
        <ext name="bnm"/>
        <name name="importantName" substring="true" ignorecase="false" />
        <resolver mime="text/x-bnm"/>
    </file>
</MIME-resolver>
